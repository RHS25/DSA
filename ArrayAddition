#include<iostream>
using namespace std;
int main(){

    int Arr[]={1,2,3,4,5,6};
    int Target=5;
    int Size=sizeof(Arr) / sizeof(Arr[0]);
     int Add=0;
     int j=0;

    for(int i=0;i<Size;i++){
        j=i+1;

        Add=Arr[i]+Arr[j];

        if(Add==Target){

            cout<<i<<" "<<j;
        }
        
        }
    
    return 0;

}
